SENSITIVITY ON
 
TITLE
   W4- DOUBLE-ENDED COLD LEG LOCA 
END
 
PARAMETER FILE ZION5.PAR
 
PARAMETER CHANGE
   ZBBN(1) = ZNOZ+0.4
   ZNBBN(1)=2.2 M
   TDFQMX = 1.E0 S
   TDCORE = 0.01 S  // LIMIT THE MAAP TIME STEP
   CNBA0=0.002
   FBESFCL=0.0025
   FBESFDC=0.0025
   TIM100=-1
   TIM101=-1
C DET Sampled Variables
 TIMELOCA = $RAVEN-TIMELOCA:-1$
 AFWOFF = $RAVEN-AFWOFF:-1$
C End DET Sampled Variables

C HYBRID Sampled Variables
C End HYBRID Sampled Variables
END

START TIME IS 0. S
END TIME IS 1000. S
C ***** END TIME IS 20000. S
PRINT INTERVAL IS 100. S

C Branching AFWOFF
WHEN TIM > AFWOFF  AND AFWOFF /= -1
 TIM101=1
 IEVNT(224)=1
END

C Branching TIMELOCA
WHEN TIM > TIMELOCA  AND TIMELOCA /= -1
   TIM100=1
   ABBN(1)=0.008
   FBBN(1)=10
   IEVNT(209)=1
   TDFQMX = 0.1 S
   TDFQMN = 0.03 S
END
 
WHEN TIM > TIMELOCA  AND TIMELOCA /= -1
   FANS/COOLERS FORCED OFF
   MBARCS0 = MBARCST
END

WHEN PPSSOLID < 1835.*6895.
 IEVNT(227)=1
 IEVNT(215)=1
END
 
IF TIM > TIMELOCA + 20 S
 TDFQMX = 2 S
 TDFQMN = 1. S
 TTPRNT = 500 S
END

IF TIM > TIMELOCA + 50 S
 TDCORE = 0.05 S
 TDFQMX = 5 S
 TDFQMN = 1. S
 TTPRNT = 500 S
END

IF TIM > TIMELOCA + 3000 S
 TTPRNT = 2000 S
END

IF TIM > TIMELOCA + 100 S
 TDCORE = 0.1 S
 TDFQMX = 10 S
 TDFQMN = 1. S
END

FUNCTION
 FBACR = MBARCS(13)/MAX(0.001E0,MWRCS(13))
 FBALP = MBARCS(12)/MAX(0.001E0,MWRCS(12))
 FBADC = MBARCS(11)/MAX(0.001E0,MWRCS(11))
END

WHEN TIM > TIMELOCA
 MBABAL = MBARCS0 +MBAINJ-MBABRK - MBARCST
 REPEAT
 SILENT
END

C Stop Simulation condition
IF (TIMER 100 > 0) OR (TIMER 101 > 0)
 TILAST=TIM
END

PLOTFIL 95
 TDOLD,ZSUBCOR,ZWV,MWSUBCR,MWEX(1)
  WESFDC,WESFCL,WWBRKN(10),WGBRKN(10),PPS
  WSTCR
  PRCS(13),PRCS(14),PRCS(11)
  ZWRCSN(9),ZWRCSN(1),ZWRCSN(2),ZWRCSN(3)
  VWRCS(11),VWRCS(13),VWRCS(14)
  WW(17),XGOUTX(3),XGOUTX(4),XGOUTX(5)
  WW(1),XGOUTX(1)
 MBARCS(11),MBARCS(12),MBARCS(13)
 MBARCS(21),MBARCS(22),MBARCS(23)
 MWRCS(11),MWRCS(12),MWRCS(13)
 FBBN(1),MWRCS(21),MWRCS(22),MWRCS(23)
 FBACR,FBALP,FBADC,IEVNT(224),ABBN(1)
 MBARCST,MBABRK,MBAINJ,MBABAL,IEVNT(209),TDFQMX,TDFQMN
END

